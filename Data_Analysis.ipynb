    "                 'stap_om_6_cal',\n",
    "                 'stap_om_7_cal',\n",
    "                 'wear_1',\n",
    "                 'wear_2',\n",
    "                 'wear_3',\n",
    "                 'wear_4',\n",
    "                 'wear_5',\n",
    "                 'wear_6',\n",
    "                 'wear_7']]\n",
    "\n",
    "data_cal_2019_original = data_cal_original[ : 91]\n",
    "data_cal_2020_original = data_cal_original[91 : 91 + data.year.value_counts()[2020]]\n",
    "data_cal_2021_original = data_cal_original[91 + data.year.value_counts()[2020] : ]"
